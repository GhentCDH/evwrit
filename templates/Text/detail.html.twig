{% extends 'base.html.twig' %}

{% block content %}
    <text-view-app id="text-view-app"
        init-urls="{{ urls }}"
        init-data="{{ data }}"
    />
{% endblock %}

{% block javascript %}
    {{ parent() }}
    {{ encore_entry_script_tags('text-view') }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('text-view') }}
{% endblock %}